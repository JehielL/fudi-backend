            }
          }
        },
        {
          "name": "Crear Reserva (HOY 20:00)",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  "var today = new Date();",
                  "var dd = String(today.getDate()).padStart(2, '0');",
                  "var mm = String(today.getMonth() + 1).padStart(2, '0');",
                  "var yyyy = today.getFullYear();",
                  "pm.collectionVariables.set('todayDate', yyyy + '-' + mm + '-' + dd);"
                ],
                "type": "text/javascript"
              }
            },
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.id) {",
                  "    pm.collectionVariables.set('bookingId', jsonData.id);",
                  "    console.log('Booking ID guardado: ' + jsonData.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"bookingDate\": \"{{todayDate}}\",\n  \"bookingTime\": \"20:00:00\",\n  \"numPeople\": 4,\n  \"restaurant\": {\n    \"id\": {{restaurantId}}\n  },\n  \"observations\": \"Mesa para cumpleanos\",\n  \"interior\": true,\n  \"tableNumber\": 5,\n  \"specialRequests\": \"Velas en la mesa\",\n  \"contactName\": \"Juan Perez\",\n  \"contactPhone\": \"+34612345678\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/bookings",
              "host": ["{{baseUrl}}"],
              "path": ["bookings"]
            }
          }
        },
        {
          "name": "Crear Reserva (MANANA 14:00)",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "exec": [
                  "var tomorrow = new Date();",
                  "tomorrow.setDate(tomorrow.getDate() + 1);",
                  "var dd = String(tomorrow.getDate()).padStart(2, '0');",
                  "var mm = String(tomorrow.getMonth() + 1).padStart(2, '0');",
                  "var yyyy = tomorrow.getFullYear();",
                  "pm.collectionVariables.set('tomorrowDate', yyyy + '-' + mm + '-' + dd);"
                ],
                "type": "text/javascript"
              }
            },
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.id) {",
                  "    pm.collectionVariables.set('bookingId', jsonData.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"bookingDate\": \"{{tomorrowDate}}\",\n  \"bookingTime\": \"14:00:00\",\n  \"numPeople\": 2,\n  \"restaurant\": {\n    \"id\": {{restaurantId}}\n  },\n  \"observations\": \"Almuerzo de trabajo\",\n  \"interior\": false,\n  \"tableNumber\": 8\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/bookings",
              "host": ["{{baseUrl}}"],
              "path": ["bookings"]
            }
          }
