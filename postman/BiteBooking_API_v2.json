      "item": [
        {
          "name": "Listar Todas las Reservas",
          "request": {
            "method": "GET",
            "header": [
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": {
              "raw": "{{baseUrl}}/bookings",
